{
  "name": "ariana",
  "version": "1.0.0",
  "description": "Modern IDE application with GitHub integration",
  "type": "module",
  "bin": {
    "ariana": "bin/cli.js"
  },
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "dev:docker": "vite build --mode development --watch",
    "build": "tsc && vite build --mode production",
    "build:staging": "tsc && vite build --mode staging",
    "build:dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev --config src-tauri/tauri.conf.dev.json",
    "tauri:prod": "tauri dev --config src-tauri/tauri.conf.prod.json",
    "tauri:build": "tauri build",
    "tauri:build:dev": "npm run build:dev && tauri build",
    "tauri:build:staging": "npm run build:staging && tauri build",
    "version:patch": "cd .. && node bump-version.mjs",
    "version:minor": "cd .. && node bump-version.mjs --minor",
    "version:major": "cd .. && node bump-version.mjs --major"
  },
  "files": [
    "bin/",
    "install.js",
    "dist/"
  ],
  "keywords": [
    "ide",
    "github",
    "tauri",
    "react",
    "typescript"
  ],
  "author": "Your Name",
  "license": "AGPL-3",
